---
export interface Props {
	title: string;
}

import Header from '../components/Header.astro'
import Footer from '../components/Footer.astro'

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-widthm initial-scale=1.0" />
		<title>{title}</title>
	</head>
	<body>
		<Header></Header>
		<slot />
		<Footer></Footer>
	</body>
</html>
